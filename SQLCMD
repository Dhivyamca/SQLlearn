SELECT * FROM patients;

select * from admissions;

select concat (first_name, ' ',last_name) as fullname FROM patients;

SELECT first_name,last_name, province_name from patients p inner join province_names pn where p.province_id= pn.province_id;

SELECT COUNT (*) from patients where YEAR(birth_date) = 2010; 

select distinct(city) from patients where province_id= 'NS';

select first_name,last_name,birth_date from patients where height >160 and weight >70;

SELECT first_name,last_name, MAX (height) from patients 

select patient_id, count (*) from admissions where patient_id= 579; 

select count (*) FROM patients p join admissions a where p.patient_id=a.patient_id;

select patient_id,admission_date,discharge_date,diagnosis,attending_doctor_id from admissions where admission_date = discharge_date ;



select patient_id,first_name,last_name,gender,birth_date,city,province_id, allergies,height,weight FROM patients WHERE patient_id IN (1,45,534,879,1000);
